{
  "name": "instafork",
  "description": "Eject npm module source into a project locally",
  "version": "0.1.0",
  "author": {
    "name": "Uber Web Platform Team"
  },
  "keywords": [
    "eject",
    "babel",
    "es6",
    "task",
    "uber-web",
    "web-platform"
  ],
  "main": "dist/instafork.js",
  "bin": {
    "instafork": "dist/bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/instafork"
  },
  "bugs": {
    "url": "https://github.com/uber-web/instafork/issues"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "find-up": "^1.0.0",
    "fs-extra": "^1.0.0",
    "globby": "^6.1.0",
    "jscodeshift": "^0.3.30",
    "jsonfile": "^2.4.0",
    "meow": "^3.6.0",
    "npm": "^4.0.5",
    "read-pkg-up": "^1.0.1",
    "resolve-from": "^2.0.0",
    "semver": "^5.3.0",
    "tar-fs": "^1.15.0",
    "uber-codemods": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.3.0",
    "babel-preset-stage-2": "^6.3.0",
    "eslint": "^2.10.0",
    "eslint-config-uber-es2015": "^2.0.0",
    "eslint-config-uber-es5": "^1.0.0",
    "flow-bin": "^0.37.0",
    "nyc": "^7.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^1.17.7",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "temp": "^0.8.3",
    "uber-licence": "^2.0.2"
  },
  "scripts": {
    "build": "babel src -d dist --copy-files -s",
    "cover": "nyc report --reporter=text-lcov",
    "flow": "flow check",
    "lint": "eslint src",
    "precommit": "npm run lint -s && npm run cover",
    "pretape": "npm run prepublish",
    "prepublish": "npm run build",
    "tape": "nyc tape dist/test/*.js | node_modules/.bin/tap-spec",
    "test": "npm run lint && npm run flow && npm run tape",
    "fast-test": "npm run build && npm run tape",
    "add-licence": "uber-licence"
  },
  "pre-commit": [
    "test"
  ],
  "pre-commit.silent": true,
  "nyc": {
    "reporter": [
      "cobertura"
    ]
  },
  "license": "MIT"
}
